{
  "name": "@trshcmpctr/client",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend React app",
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "client"
  },
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": "./dist/manifest.json",
    "./paths": "./lib/paths.js"
  },
  "main": "./dist/manifest.json",
  "files": [
    "dist",
    "lib/paths.js"
  ],
  "scripts": {
    "build": "webpack",
    "build:production": "webpack --env production",
    "build:watch-webpack": "nodemon --exec 'webpack' --watch webpack.config.js --watch babel.config.js --watch babel.config.lib.js",
    "clean": "rm -rf dist",
    "cy:open": "cypress open --config-file=cypress/cypress.config.ts --e2e --browser=chrome",
    "cy:run": "cypress run --config-file=cypress/cypress.config.ts",
    "lint": "eslint --max-warnings=0 .",
    "lint:md": "markdownlint-cli2 --config node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc",
    "serve": "webpack serve",
    "serve:watch-webpack": "nodemon --exec 'webpack serve' --watch webpack.config.js --watch babel.config.js --watch babel.config.lib.js",
    "start": "webpack serve --open",
    "start:production": "webpack serve --open --env production",
    "test": "jest --reporters=@trshcmpctr/jest-stdout-reporter --silent",
    "test:cypress": "start-server-and-test serve http://localhost:8080 cy:run",
    "test:debug": "NODE_OPTIONS=--inspect jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "type-check": "tsc",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "axios": "~1.13.2",
    "core-js": "~3.47.0",
    "prop-types": "~15.8.1",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "react-router": "~7.11.0"
  },
  "devDependencies": {
    "@babel/core": "~7.28.5",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-modules-commonjs": "~7.27.1",
    "@babel/preset-env": "~7.28.5",
    "@babel/preset-react": "~7.28.5",
    "@babel/preset-typescript": "~7.28.5",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/jest-dom": "~6.9.1",
    "@testing-library/react": "~16.3.0",
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/eslint-config-node": "workspace:*",
    "@trshcmpctr/eslint-config-react": "workspace:*",
    "@trshcmpctr/eslint-config-typescript": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "@types/jest": "~30.0.0",
    "@types/node": "~20.19.25",
    "@types/prop-types": "~15.7.12",
    "@types/react": "~19.2.5",
    "@types/react-dom": "~19.2.3",
    "babel-jest": "~30.2.0",
    "babel-loader": "~10.0.0",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "css-loader": "~7.1.1",
    "cypress": "13.17.0",
    "discord-api-types": "~0.38.33",
    "eslint": "~9.39.1",
    "eslint-plugin-cypress": "~5.2.0",
    "eslint-plugin-n": "~17.23.1",
    "eslint-plugin-testing-library": "~7.15.3",
    "globals": "~16.5.0",
    "html-webpack-plugin": "~5.6.0",
    "jest": "~30.2.0",
    "jest-environment-jsdom": "~30.2.0",
    "jest-serializer-path": "~0.1.15",
    "markdownlint-cli2": "~0.20.0",
    "nodemon": "~3.1.0",
    "start-server-and-test": "~2.1.2",
    "style-loader": "~4.0.0",
    "terser-webpack-plugin": "~5.3.1",
    "typescript": "~5.9.3",
    "webpack": "~5.104.1",
    "webpack-cli": "~6.0.1",
    "webpack-dev-server": "~5.2.2",
    "webpack-manifest-plugin": "~6.0.1"
  }
}
